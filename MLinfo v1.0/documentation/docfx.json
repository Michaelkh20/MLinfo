{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Controllers/**/*.cs",
            "Data/**/*.cs",
            "Models/**/*.cs"
          ]
        }
      ],
      "globalNamespaceId": "Global",
      "filter": "filter_config.yml",
      "dest": "api"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "ML Info Docs",
      "_appName": "Machine Learning Articles",
      "_appFooter": "Generated by DocFX",
      "_copyrightFooter": "Â© 2022 Elv1z. All rights reserved.",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_gitUrlPattern": "github"
    },
    "template": [
      "default"
    ],
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      },
      {
        "src": "api",
        "files": [
          "*.yml"
        ],
        "dest": "api"
      },
      {
        "src": "manual",
        "files": [
          "toc.yml",
          "*.md"
        ],
        "dest": "manual"
      }
    ],
    "overwrite": [
      {
        "src": "..",
        "files": [
          "Controllers/**/*.md",
          "Data/**/*.md",
          "Models/**/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [

        ]
      }
    ],
    "sitemap": {
      "baseUrl": "https://michaelkh20.github.io/MLinfo/",
      "changefreq": "weekly",
      "fileOptions": {
        "api/*": {
          "changefreq": "daily"
        }
      }
    },
    "xref": [
      "https://germandilio.github.io/MLinfo/xrefmap.yml"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "../_site"
  }
}
